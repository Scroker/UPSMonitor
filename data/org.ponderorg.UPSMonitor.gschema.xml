<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="upsmonitor">
	<schema id="org.ponderorg.UPSMonitor" path="/org/ponderorg/UPSMonitor/">
    <key name="run-in-background" type="b">
      <default>false</default>
    </key>
	  <key name="run-at-boot" type="b">
      <default>false</default>
    </key>
	  <key name="prefer-dark" type="b">
      <default>false</default>
    </key>
	  <key name="retry-max" type="i">
      <default>0</default>
    </key>
	  <key name="polling-interval" type="i">
      <default>1</default>
    </key>
  </schema>
</schemalist>
