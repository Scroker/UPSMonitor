<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <requires lib="Adw" version="1.0"/>
  <template class="UpsPreferencesPage" parent="AdwNavigationPage">
    <property name="title">UPS Page</property>
    <property name="tag">ups_page</property>
    <property name="child">
      <object class="AdwToolbarView">
        <child type="top">
          <object class="AdwHeaderBar">
            <property name="title-widget">
              <object class="AdwWindowTitle" id="page_title"/>
            </property>
          </object>
        </child>
        <child>
          <object class="GtkBox">
            <property name="orientation">vertical</property>
            <child>
              <object class="AdwClamp">
                <property name="margin-top">10</property>
                <property name="margin-bottom">10</property>
                <property name="margin-start">15</property>
                <property name="margin-end">15</property>
                <child>
                  <object class="GtkListBox">
                    <property name="selection-mode">none</property>
                    <style>
                      <class name="boxed-list"/>
                    </style>
                    <child>
                      <object class="AdwActionRow" id="battery_row">
                        <property name="title">Battery</property>
                        <child type="prefix">
                          <object class="GtkImage" id="battery_image">
                            <style>
                              <class name="icon-dropshadow"/>
                            </style>
                          </object>
                        </child>
                        <child type="suffix">
                          <object class="GtkLevelBar" id="battery_level_bar">
                            <property name="margin-top">20</property>
                            <property name="margin-bottom">20</property>
                            <property name="hexpand">true</property>
                            <property name="vexpand">true</property>
                            <property name="max-value">100</property>
                            <property name="min-value">0</property>
                            <property name="value">0</property>
                          </object>
                        </child>
                      </object>
                    </child>
                    <child>
                      <object class="AdwExpanderRow">
                        <property name="title">Voltage</property>
                        <child type="prefix">
                          <object class="GtkImage">
                            <property name="icon-name">camera-flash-symbolic</property>
                            <style>
                              <class name="icon-dropshadow"/>
                            </style>
                          </object>
                        </child>
                        <child type="suffix">
                          <object class="GtkLabel" id="voltage_label">
                            <style>
                              <class name="heading"/>
                            </style>
                          </object>
                        </child>
                        <child>
                          <object class="AdwActionRow">
                            <property name="title">Input voltage</property>
                            <child type="prefix">
                              <object class="GtkImage">
                                <property name="icon-name">arrow4-left-symbolic</property>
                              </object>
                            </child>
                            <child type="suffix">
                              <object class="GtkLabel" id="input_voltage_label">
                                <style>
                                  <class name="heading"/>
                                </style>
                              </object>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="AdwActionRow">
                            <property name="title">Output voltage</property>
                            <child type="prefix">
                              <object class="GtkImage">
                                <property name="icon-name">arrow4-right-symbolic</property>
                              </object>
                            </child>
                            <child type="suffix">
                              <object class="GtkLabel" id="output_voltage_label">
                                <style>
                                  <class name="heading"/>
                                </style>
                              </object>
                            </child>
                          </object>
                        </child>
                      </object>
                    </child>
                    <child>
                      <object class="AdwActionRow">
                        <property name="title">Frequency</property>
                        <child type="prefix">
                          <object class="GtkImage">
                            <property name="icon-name">charge-symbolic</property>
                            <style>
                              <class name="icon-dropshadow"/>
                            </style>
                          </object>
                        </child>
                        <child type="suffix">
                          <object class="GtkLabel" id="frequency_label">
                            <style>
                              <class name="heading"/>
                            </style>
                          </object>
                        </child>
                      </object>
                    </child>
                    <child>
                      <object class="AdwActionRow">
                        <property name="title">Current</property>
                        <child type="prefix">
                          <object class="GtkImage">
                            <property name="icon-name">power-profile-performance-symbolic</property>
                            <style>
                              <class name="icon-dropshadow"/>
                            </style>
                          </object>
                        </child>
                        <child type="suffix">
                          <object class="GtkLabel" id="current_label">
                            <style>
                              <class name="heading"/>
                            </style>
                          </object>
                        </child>
                      </object>
                    </child>
                    <child>
                      <object class="AdwActionRow">
                        <property name="title">Host</property>
                        <child type="prefix">
                          <object class="GtkImage" id="host_image">
                            <style>
                              <class name="icon-dropshadow"/>
                            </style>
                          </object>
                        </child>
                        <child type="suffix">
                          <object class="GtkLabel" id="host_label">
                            <style>
                              <class name="heading"/>
                            </style>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="AdwClamp">
                <property name="margin-top">10</property>
                <property name="margin-bottom">10</property>
                <property name="margin-start">15</property>
                <property name="margin-end">15</property>
                <child>
                  <object class="GtkListBox">
                    <property name="selection-mode">none</property>
                    <style>
                      <class name="boxed-list"/>
                    </style>
                    <child>
                      <object class="AdwActionRow">
                        <property name="title">Notifications</property>
                        <property name="activatable">true</property>
                        <child type="prefix">
                          <object class="GtkImage">
                            <property name="name">logo</property>
                            <property name="icon-name">org.gnome.Settings-notifications-symbolic</property>
                            <style>
                              <class name="icon-dropshadow"/>
                            </style>
                          </object>
                        </child>
                        <child type="suffix">
                          <object class="GtkImage">
                            <property name="icon-name">go-next-symbolic</property>
                          </object>
                        </child>
                      </object>
                    </child>
                    <child>
                      <object class="AdwActionRow">
                        <property name="title">Info</property>
                        <property name="activatable">true</property>
                        <child type="prefix">
                          <object class="GtkImage">
                            <property name="icon-name">info-symbolic</property>
                            <style>
                              <class name="icon-dropshadow"/>
                            </style>
                          </object>
                        </child>
                        <child type="suffix">
                          <object class="GtkImage">
                            <property name="icon-name">go-next-symbolic</property>
                          </object>
                        </child>
                      </object>
                    </child>
                    <child>
                      <object class="AdwActionRow">
                        <property name="title">Settings</property>
                        <property name="activatable">true</property>
                        <child type="prefix">
                          <object class="GtkImage">
                            <property name="icon-name">settings-symbolic</property>
                            <style>
                              <class name="icon-dropshadow"/>
                            </style>
                          </object>
                        </child>
                        <child type="suffix">
                          <object class="GtkImage">
                            <property name="icon-name">go-next-symbolic</property>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </property>
  </template>
</interface>
